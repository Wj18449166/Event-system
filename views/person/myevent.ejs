<!--home.ejs-->
<h5>Registered events</h5>
<div class="row">

    <% event.forEach( function(model){ %>
    <div class="card col-12 col-sm-12 col-lg-4 col-xl-4 ">
        <div class="card-body">
            <img class="img-fluid img-thumbnail" src="<%= model.imgURL %>">
            <h5><a href="/Event/detail/<%= model.id %>">
                    <%= model.name %></a></h5>
            <div class="media-body">
                <ul class="card list-group list-group-flush">
                    <li class="list-group-item"><b>Date: </b>
                        <%= model.eventDate %>
                    </li>
                    <li class="list-group-item"><b>Time: </b>
                        <%= model.time %>
                    </li>
                    <li class="list-group-item"><b>Venue: </b>
                        <%= model.venue %>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <% }); %>
</div>